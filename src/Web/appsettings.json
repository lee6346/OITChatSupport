{
  "EmailMessage": {
    "SystemError": "System Failure"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Serilog": {
    "TableName": "Logs",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    }
  },
  "DirectLineApi": {
    "BaseUri": "https://directline.botframework.com",
    "EndPoints": {
      "Token": "v3/directline/tokens/generate",
      "TokenRefresh": "v3/directline/tokens/refresh",
      "Conversation": "v3/directline/conversations"
    }
  }
}